<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="local.CanReceiver">
    <method name="setData">
      <arg name="my_speed" type="d" direction="in"/>
      <arg name="my_rpm" type="d" direction="in"/>
      <arg name="my_battery" type="d" direction="in"/>
      <arg name="my_gear" type="s" direction="in"/>
    </method>
  </interface>
</node>
