<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="local.CanDataReceiver">
    <property name="speed" type="d" access="read"/>
    <property name="rpm" type="d" access="read"/>
    <signal name="speedChanged">
      <arg name="newSpeed" type="d" direction="out"/>
    </signal>
    <signal name="rpmChanged">
      <arg name="newRpm" type="d" direction="out"/>
    </signal>
  </interface>
</node>
